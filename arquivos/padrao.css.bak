/****************** MODAL *********************/
.modal {
  display: none; 
  position: fixed; 
  z-index: 1000; 
  left: 0;
  top: 0;
  width: 100%; 
  height: 100%; 
  background-color: rgba(86, 84, 84, 0.4);
}
.modal-content {
  background-color: var(--fundo-menu);
	color: var(--cor-menu);
  margin: 1% auto;
  padding: 5px;
  border: 1px solid #888;
  width: 99%;
	max-width: 99%;
	max-height: 98%;
	overflow: auto;
	z-index: 10000;
}
.modal-content label, .modal-content td {
	color: var(--branco);
}
.close {
	color: var(--branco);
	background-color: var(--cor-titulo);
  float: right;
  font-size: 15px;
  font-weight: bold;
	padding: 5px;
	border-radius: 10px;
	margin-left: 2px;
}

.barraTop {
	position: fixed;
	left: 0px;
	font-size: 20px;
	font-weight: bold;
	background-color: var(--cor-menu);
}

.fechar {
	position: fixed;
	top: 5px;
	right: 10px;
	color: var(--branco);
	background-color: var(--cor-titulo);
  font-size: 15px;
  font-weight: bold;
	padding: 5px;
	border-radius: 10px;
	margin-left: 2px;
}

.gravar {
	position: fixed;
	bottom: 30px;
	right: 20px;
	color: var(--branco);
	background-color: var(--cor-titulo);
  font-size: 15px;
  font-weight: bold;
	padding: 5px;
	border-radius: 10px;
	margin-left: 2px;
}

.close:active, .botao:active {
  background-color: #3e8e41;
  box-shadow: 0 5px #666;
  transform: translateY(4px);
}
.botao:hover,
.botao:focus,
.fechar:hover,
.fechar:focus,
.gravar:hover,
.gravar:focus,
.close:hover,
.close:focus {
  color: black;
  text-decoration: none;
  cursor: pointer;
}

#textoMensagem, #textoAviso {
	font-size: 16px;
}

span.tituloEdicao, .modalTitulo {
	float: left;
	font-size: 15px;
