/* Padrão menu e aplicação */
*,
*:before,
*:after {
  box-sizing: border-box;
}
li { list-style-type: none;	list-style-position: outside; }
/****************** TABELA.PADRAO *********************/
/*table.consulta th, table.consulta td { font-size: 12px; }*/
table.padrao th {
	font-weight: bold;
	background-color: var(--fundo-menu);
	color: var(--cor-menu);
	/*
	background-color: var(--fundo-tabela-padrao);
	text-align: center;
	border:  1px solid var(--fundo-menu);
	*/
	/*border-radius: 20px;*/
	font-size: 14px;
	font-weight: bold;
}
table.padrao th.tipo_fluxo, table.padrao th.tipo_fluxo_negativo {
	font-weight: bold;
	/*
	text-align: center;
	border:  1px solid var(--fundo-menu);
	background-color: var(--fundo-tabela-padrao);
	*/
	/*border-radius: 20px;*/
	color: var(--cor-menu);
	background-color: var(--fundo-menu);
	font-size: 14px;
	font-weight: bold;
}
table.padrao th.tipo_fluxo_negativo {
	color: var(--negativo);
	/*border:  2px dashed var(--fundo-menu);*/
}
table.padrao td {
	color: var(--fundo-menu);
	font-size: 12px;
	padding: 5px;
	margin:  0px;
	height: 20px;
/*
	border: 1px solid var(--preto);
	line-height: 20px;
	border-radius: 10px;
*/
}

table.padrao tr:nth-child(odd), .tr_odd {
	background-color: #E8E8E8;
}

table.padrao td label {
	font-size: 12px;
}
table.padrao td.positivo, table.padrao th.positivo {
	/*background-color: var(--branco);*/
	color: var(--positivo-color);
}
table.padrao td.positivo_total, table.padrao th.positivo_total {
	/*background-color: var(--branco);*/
	color: var(--positivo-total-color);
}
table.padrao td.negativo, table.padrao th.negativo, div.negativo {
	/*background-color: var(--negativo);*/
	color: var(--negativo-color);
	font-weight: bold;
}
table.padrao td.acao a {
	cursor: pointer;
	width: 100%;
	height: 100%;
	border: 0;
	color: var(--fundo-menu);
	font-weight: bold;
}
table.padrao td.acao, table.padrao th.acao {
	/*cursor: pointer;*/
	/*background-color: var(--cor-titulo);*/
	/*color: var(--branco);*/
	background: transparent;
	text-align: center;
	font-weight: bold;
	border: none; /*1px solid var(--branco) ;*/
	width: 5%;
}
table.padrao td a {
	text-decoration: none;
	padding: 2px;
	border: 0;
	font-weight: bold;
	color: var(--cor-menu);
}
table.padrao td.botao {
	text-align: center;
	cursor: pointer;
	background-color: var(--cor-titulo);
	color:  var(--branco);
	border: 1px solid var(--branco);
}

table.formaPagto td, table.formaPagto th {
	border: 1px solid var(--branco);
	color: var(--branco);
	font-size: 12px;
}

table.formaPagto td.nada, table.formaPagto th.nada {
	border: none;
}

