//----- FUNÇÕES GENÉRICAS --------------------------------------------------------------------------------------------------//--------------------------------------------------------------------------------------------------	function teclado_numerico_muda_seq(p_indice){//--------------------------------------------------------------------------------------------------		v_qtd = campos_teclado_numerico.length;		for(var i=0; i < v_qtd; i++) {			$("#"+campos_teclado_numerico[i]).addClass("class_input");		}		v_atual = $("#indice_teclado_numerico").val();		if (p_indice == -1) {			v_atual--;			if (v_atual < 0) {				v_atual = v_qtd -1;			}		}		if (p_indice == 1) {			v_atual++;			if (v_atual >= v_qtd) {				v_atual = 0;			}		}		v_campo = campos_teclado_numerico[v_atual];		v_tipo = tipos_teclado_numerico[v_atual];		$("#campo_teclado_numerico").val(v_campo);		$("#tipo_valor_teclado_numerico").val(v_tipo);		$("#indice_teclado_numerico").val(v_atual).trigger("change");		// $("#"+v_campo).toggleClass("display");	}//--------------------------------------------------------------------------------------------------	function teclado_numerico_clique(p_valor){//--------------------------------------------------------------------------------------------------		var number = p_valor;		v_campo_id = $("#campo_teclado_numerico").val();		var v_campo_teclado_numerico = "#" + v_campo_id;		var v_tipo_valor_teclado_numerico = $("#tipo_valor_teclado_numerico").val();		var v_tipo_teclado = tipos_teclado_numerico[$("#indice_teclado_numerico").val()];		if (number == "excluir") {			if (v_tipo_teclado == "P") {				$(v_campo_teclado_numerico).val($(v_campo_teclado_numerico).val().substr(0, $(v_campo_teclado_numerico).val().length - 1)).focus();							} else {				$(v_campo_teclado_numerico).html($(v_campo_teclado_numerico).html().substr(0, $(v_campo_teclado_numerico).html().length - 1)).focus();							}		} else {			var str = number;			var res = str.replace(/\D/g, "");			if (res) {				if (v_tipo_teclado == "P") {					$(v_campo_teclado_numerico).val($(v_campo_teclado_numerico).val() + number).focus();				} else {										$(v_campo_teclado_numerico).html($(v_campo_teclado_numerico).html() + number).focus();				}			}		}		if (v_tipo_valor_teclado_numerico == "D") {			formatarMoedaHtml(document.getElementById(v_campo_id));		}	}//--------------------------------------------------------------------------------------------------	function muda_dia(p_campo, p_dia){//--------------------------------------------------------------------------------------------------    var dia = new Date($("#"+p_campo).val());    dia.setDate(dia.getDate() + parseInt(p_dia));		document.getElementById(p_campo).valueAsDate = dia;		mostra("telaEdicao");		mostra("telaEdicaoTab");	}//--------------------------------------------------------------------------------------------------	function calcula_mes_ano(p_mes, p_ano, p_indice){//--------------------------------------------------------------------------------------------------		if (p_indice == 1) {			if (v_mes == 12) {				v_ano++;				v_mes = 1;			} else {				v_mes++;			}		}		if (p_indice == -1) {			if (v_mes == 1) {				v_ano--;				v_mes = 12;			} else {				v_mes--;			}		}		return v_ano + "-" + (v_mes < 10 ? "0"+ v_mes : v_mes);	}//--------------------------------------------------------------------------------------------------	function muda_mes(p_campo, p_indice){//--------------------------------------------------------------------------------------------------    var v_mes_ano = $("#"+p_campo).val();		v_ano = parseInt(v_mes_ano.substr(0, 4));		v_mes = parseInt(v_mes_ano.substr(5, 2));				$("#"+p_campo).val(calcula_mes_ano(v_mes, v_ano, p_indice));	}//--------------------------------------------------------------------------------------------------function muda_mes_ano(p_indice){//--------------------------------------------------------------------------------------------------	v_mes = parseInt(el('p_mes').value);	v_ano = el('p_ano').value;	v_mes_ano = calcula_mes_ano(v_mes, v_ano, p_indice);	el('p_mes').value = v_mes_ano.substr(5, 2);	el('p_ano').value = v_mes_ano.substr(0, 4);}//--------------------------------------------------------------------------------------------------function temporizadorAvisoFrm(p_frm, p_tipo) {//--------------------------------------------------------------------------------------------------	if (p_tipo != 1) {		if (p_frm != '') {			$("#"+p_frm).submit();		} else {			location.reload(true);		}	} else {		$('#barraAviso').hide();	}}//--------------------------------------------------------------------------------------------------function processaModalEdicao(p_mensagem) {//--------------------------------------------------------------------------------------------------	v_frm = $("#nome_frm_modal").val();	v_tipo = $("#tipo_modal").val();	$("#tipo_modal").val(2);	$("#modalEdicao").hide();	aviso(p_mensagem);	var tempoAviso = setTimeout(function(){ 		temporizadorAvisoFrm(v_frm, v_tipo);	}, tempoEsperaAviso);	$("#contentAviso").click(function(){		clearTimeout(tempoAviso);		temporizadorAvisoFrm(v_frm, v_tipo);  });}//--------------------------------------------------------------------------------------------------function tituloModalEdicao(p_titulo) {//--------------------------------------------------------------------------------------------------	$("#modalTituloConteudo").html(p_titulo);}//--------------------------------------------------------------------------------------------------function gravarModalEdicao(p_frm, p_url) {//--------------------------------------------------------------------------------------------------	var dados = $('#' + p_frm).serialize();	$.ajax({			type : 'POST',			dataType: 'TEXT',			url  : p_url,			data: dados,			success :  function(data){				processaModalEdicao(data);			}	});	}//--------------------------------------------------------------------------------------------------function modalFechar() {//--------------------------------------------------------------------------------------------------	conteudo("modalConteudo", "");	esconde("modalEdicao");		nome_frm = $("#nome_frm_modal").val();	if (nome_frm != "") {		$("#" + nome_frm).submit();	}}//--------------------------------------------------------------------------------------------------function excluir_id(pIndice, pTabela, pFrm) {//--------------------------------------------------------------------------------------------------	$("#p_exclusao_opcao").val("E");	$("#p_exclusao_id").val(pIndice);	$("#p_exclusao_tabela").val(pTabela);	var dados = $('#frm_exclusao').serialize();	$.ajax({			type : 'POST',			dataType: 'TEXT',			contentType: 'application/x-www-form-urlencoded; charset=UTF-8', //ISO-8859-1			url  : "/apps/include/excluir_id.php",			data: dados,			success :  function(data){				aviso(data);				tempoAviso = setTimeout(function(){					temporizadorAvisoFrm(pFrm, 2);				}, tempoEsperaAviso);									$("#contentAviso").click(function(){					clearTimeout(tempoAviso);					temporizadorAvisoFrm(pFrm, 2);				});			}	});		}//--------------------------------------------------------------------------------------------------function abre_parcela() {//--------------------------------------------------------------------------------------------------	$("#div_parcelas").show();}//--------------------------------------------------------------------------------------------------function seleciona_parcela(p_valor) {//--------------------------------------------------------------------------------------------------	$("#span_parcela").html(p_valor);	$("#div_parcelas").hide();}