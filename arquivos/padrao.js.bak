var hTxt;var mobile  = 0;var tempoEsperaAviso = 2000;var ls 			= localStorage;var vBranco = '&nbsp;';var meses 	= Array("Janeiro", "Fevereiro", "Março", "Abril", "Maio", "Junho", "Julho", "Agosto", "Setembro", "Outubro", "Novembro", "Dezembro");var db			= [];var config	= []; // 0 - Titulo, 1 - tela, 2 - Base, 3 - array com dados, 4 - Campos [nome, label, default]var op			= "";// var site 		=	"http://csn.orgfree.com/Site/";var mensagemAtual			= 0;var indiceAtualizacao = -1;var br = "<br>";//----- FUNÇÕES GENÉRICAS --------------------------------------------------------------------------------------------------function el(pID) { return document.getElementById(pID); } // Retorna elemento por IDfunction eln(pName) { return document.getElementsByName(pName); } // Retorna elemento por Namefunction conteudo(pID, pConteudo) { el(pID).innerHTML = pConteudo; } // Atualiza pId com pConteudofunction limpaConteudo() { conteudo("conteudo", ""); }function mostra(pID) { el(pID).style.display = "block"; } 	// Mostra/Esconde elemento htmlfunction mostraTr(pID) { el(pID).style.display = "table-row"; } 	// Mostra/Esconde elemento htmlfunction esconde(pID) { el(pID).style.display = 'none';	} 	// Esconde elemento htmlfunction ph(pConteudo) { hTxt += pConteudo; } // Prepara conteúdo HTML para uso em determinada Elementofunction phi() { hTxt = ""; } // Prepara conteúdo HTML para uso em determinada Elementofunction valorId(pID, pValor) { // Altera valor de um objeto identificado por ID	if (pValor == '') {		return el(pID).value;	} else {		el(pID).value = pValor;  	}}function valorE(pID) { return el(pID).value; } // Retorna valor de um objeto identificado por IDfunction preparaValorLS(pID) {	vlP = vlE(pID);	vlP = vlP.replace(".", "");	return vlP;}function ih(pID) { return el(pID).innerHTML; }function htmlId(pID) { el(pID).innerHTML = hTxt; }function sp(pTexto) { return "<span>"+pTexto+"</span>"; }function span(pTexto, pOpcoes) { return "<span "+pOpcoes+">"+pTexto+"</span>"; }// Funções de Tabela dentro de um div/select HTMLfunction pTable(pConteudo) { hTxt += (pConteudo == '' ? '</table>' : '<table ' + pConteudo + '>') ; } // Table	function ptd(pConteudo, pOpcoes) { hTxt += '<td' + (pOpcoes='' ? '' : ' ' + pOpcoes) +'>' + pConteudo + '</td>'; } // TDfunction pth(pConteudo, pOpcoes) { hTxt += '<th' + (pOpcoes='' ? '' : ' ' + pOpcoes) +'>' + pConteudo + '</th>'; } // THfunction ptr() { hTxt += '</tr>'; } // TRfunction copiaId(pID) {	el(pID).focus();	el(pID).select();	document.execCommand("copy");}//--------------------------------------------------------------------------------------------------function menu(p_opcao) {//--------------------------------------------------------------------------------------------------	el("op_menu").value = p_opcao;	el("frm_menu").submit();}//--------------------------------------------------------------------------------------------------function ptMoeda(pValor) { //--------------------------------------------------------------------------------------------------	vSinal = (pValor < 0 ? "-" : "");	vValor = parseFloat(pValor);	vValor = (vValor < 0 ? vValor * (-1) : vValor);	ptd(vSinal  + formatoMoeda(pValor), 'align=right');} // Item tabela com conteudo de valor em moeda//--------------------------------------------------------------------------------------------------function ptdAcao(pConteudo) { ptd(pConteudo, 'class=acao'); } // Item tabela com conteudo de Acao Javascript//--------------------------------------------------------------------------------------------------// fim Funções de Tabela//--------------------------------------------------------------------------------------------------function valorHoje() {//--------------------------------------------------------------------------------------------------	var date = new Date();	var day = date.getDate();	var month = date.getMonth() + 1;	var year = date.getFullYear();	anoAtual = parseInt(year);	if (month < 10) month = "0" + month;	if (day < 10) day = "0" + day;	var hoje = year + "-" + month + "-" + day;	return hoje;}//--------------------------------------------------------------------------------------------------function mesHoje() {//--------------------------------------------------------------------------------------------------	var date = new Date();	var month = date.getMonth() + 1;	var year = date.getFullYear();	anoAtual = parseInt(year);	if (month < 10) month = "0" + month;	var mes = year + "-" + month;	return mes;}//--------------------------------------------------------------------------------------------------function hojeHora() {//--------------------------------------------------------------------------------------------------	var dataAtual = new Date();	var horas = dataAtual.getHours();	horas = (horas < 10 ? "0" + horas : horas);	var minutos = dataAtual.getMinutes();		minutos = (minutos < 10 ? "0" + minutos : minutos);	var agora = horas + ":" + minutos;	return agora;}//--------------------------------------------------------------------------------------------------function formatarMoeda(pElemento) {//--------------------------------------------------------------------------------------------------	var elemento = pElemento; 	var valor = elemento.value;		valor += '';	valor = parseInt(valor.replace(/[\D]+/g,''));	valor += '';	valor = valor.replace(/([0-9]{2})$/g, ",$1");	if (valor.length > 6) {		valor = valor.replace(/([0-9]{3}),([0-9]{2}$)/g, ".$1,$2");	}	elemento.value = valor;}//--------------------------------------------------------------------------------------------------function formatarMoedaHtml(pElemento) {//--------------------------------------------------------------------------------------------------	var elemento = pElemento; 	var valor = elemento.innerHTML;		valor += '';	valor = parseInt(valor.replace(/[\D]+/g,''));	valor += '';	valor = valor.replace(/([0-9]{2})$/g, ",$1");	if (valor.length > 6) {		valor = valor.replace(/([0-9]{3}),([0-9]{2}$)/g, ".$1,$2");	}	elemento.innerHTML = valor;}//--------------------------------------------------------------------------------------------------function formatoMesAno(pData) {//--------------------------------------------------------------------------------------------------	return pData.substr(5,2) + '/' + pData.substr(0,4);}//--------------------------------------------------------------------------------------------------function formatoData(pData) {//--------------------------------------------------------------------------------------------------	return pData.substr(8,2) + '/' + pData.substr(5,2) + '/' + pData.substr(0,4);}//--------------------------------------------------------------------------------------------------function formatoMoeda(pNumero) {//--------------------------------------------------------------------------------------------------	var numero = parseFloat(pNumero.replace(",", "."));	var numero = numero.toFixed(2).split('.');  numero[0] = numero[0].split(/(?=(?:...)*$)/).join('.');  return numero.join(',');}//--------------------------------------------------------------------------------------------------function escondeElementoClasse(pElemento, pClasse) {//--------------------------------------------------------------------------------------------------	var elm = document.getElementsByTagName(pElemento);	for (var i = 0; i < elm.length; i++) {		if (elm[i].className == pClasse) {			esconde(elm[i].id);		}	}}//--------------------------------------------------------------------------------------------------function mostraElementoClasse(pElemento, pClasse) {//--------------------------------------------------------------------------------------------------	var elm = document.getElementsByTagName(pElemento);	for (var i = 0; i < elm.length; i++) {		if (elm[i].className == pClasse) {			if (pElemento == "tr") {				el(elm[i].id).style.display = "table-row";			} else {				mostra(elm[i].id);							}		}	}}//--------------------------------------------------------------------------------------------------function mensagem(pTexto) { //--------------------------------------------------------------------------------------------------	mostra("barraMensagem");	conteudo("textoMensagem", pTexto);}//--------------------------------------------------------------------------------------------------function aviso(pTexto) { //--------------------------------------------------------------------------------------------------	mostra("barraAviso");	conteudo("textoAviso", pTexto);}//--------------------------------------------------------------------------------------------------function mensagemAppPadrao(pTexto) {//--------------------------------------------------------------------------------------------------	mensagem(pTexto);}//--------------------------------------------------------------------------------------------------function limpaConteudo() {//--------------------------------------------------------------------------------------------------	conteudo("conteudo", "");}//--------------------------------------------------------------------------------------------------Array.prototype.findFirstSubstring = function(s) {//--------------------------------------------------------------------------------------------------	for(var i = 0; i < this.length;i++) {		if(this[i].indexOf(s) !== -1) { return i; }	}	return -1;}//--------------------------------------------------------------------------------------------------function verificaMobile() {//--------------------------------------------------------------------------------------------------	browser = window.navigator.userAgent;	browser = browser.toLowerCase() ;	posAndroid = browser.indexOf("android");	posIphone = browser.indexOf("iphone");	if (posAndroid > 0 || posIphone > 0) {mobile = 1;}}//--------------------------------------------------------------------------------------------------function recuperaLS(pBase) {//--------------------------------------------------------------------------------------------------	c = ls.getItem(pBase);	return (c != null) ? JSON.parse(c) : Array();}//--------------------------------------------------------------------------------------------------function gravarLS() {//--------------------------------------------------------------------------------------------------	ls.setItem(config[op][2], JSON.stringify(db));	db.sort();	config[op][3] = db;}//--------------------------------------------------------------------------------------------------function app(pPagina) {	base = window.location.href;	base = base.substr(0, base.lastIndexOf('/'));	window.location.href = base + '/'+pPagina+'.html';}//--------------------------------------------------------------------------------------------------function branco(pQtd=1) {	vRetorno = vBranco;	if (pQtd > 1) { for(var i=1; i < pQtd; i++) { vRetorno += vBranco; } }	return vRetorno;}//--------------------------------------------------------------------------------------------------function ajaxDivForm(p_php, p_div, p_formulario) {// Recupera dados remotos//--------------------------------------------------------------------------------------------------	var v_frm = $("#" + p_formulario);	var v_div = "#" + p_div;	v_url = (p_php == "" ? location.href : p_php);	//alert(v_frm.serialize());	$.ajax(		{			url: v_url, 			contentType: 'application/x-www-form-urlencoded; charset=UTF-8', //ISO-8859-1			type: 'post',			dataType: 'html',			data: v_frm.serialize(),			success: function(data) {				$(v_div).html(data);		},		beforeSend: function(jqXHR) {        jqXHR.overrideMimeType('text/html;charset=UTF-8');    },		onFailure: function() {				alert('Erro: Tente mais tarde.');			}		}	)}//--------------------------------------------------------------------------------------------------function myNavMenu() {//--------------------------------------------------------------------------------------------------	var x = document.getElementById("myTopnav");	if (x.className === "topnav") {		x.className += " responsive";	} else {		x.className = "topnav";		if (verificaMobile() == 1) {			$(".dropdown-content").hide();					}	}}